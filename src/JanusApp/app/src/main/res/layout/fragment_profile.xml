<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".profileFragment">

    <ImageButton
        android:id="@+id/imageButton_MisInversiones"
        android:layout_width="150dp"
        android:layout_height="138dp"
        android:layout_marginTop="80dp"
        android:backgroundTint="#00FFFFFF"
        android:contentDescription="@string/Folder_My_Projects_Profile"
        android:src="@mipmap/ic_profile_myinvest"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton_MisProyectos"
        android:layout_width="150dp"
        android:layout_height="138dp"
        android:layout_marginTop="40dp"
        android:backgroundTint="#00FFFFFF"
        android:contentDescription="@string/Folder_My_Projects_Profile"
        android:src="@mipmap/ic_profile_mi_project"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton_MisInversiones" />

    <TextView
        android:id="@+id/textView_MisInversiones"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Folder_Invest_Profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton_MisInversiones" />

    <TextView
        android:id="@+id/textView_MisProyectos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Folder_My_Projects_Profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton_MisProyectos" />
</androidx.constraintlayout.widget.ConstraintLayout>